{
  "name": "nuxt-meilisearch",
  "version": "0.0.8",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xlanex6/nuxt-meilisearch.git"
  },
  "author": "Alex Duval",
  "contributors": [],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "docs": "cd docs && pnpm dev",
    "release": "nuxt-module-build && standard-version && git push --follow-tags && npm publish",
    "lint": "eslint --ext .js,.ts,.vue"
  },
  "dependencies": {
    "@meilisearch/instant-meilisearch": "0.7.1",
    "@nuxt/kit": "3.0.0-rc.2",
    "instantsearch.css": "^7.4.5",
    "vue-instantsearch": "4.3.3"
  },
  "devDependencies": {
    "@nuxt/module-builder": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "axios": "^0.27.2",
    "eslint": "latest",
    "nuxt": "3.0.0-rc.4",
    "standard-version": "9.3.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://nuxt-meilisearch-docs.netlify.app/",
  "directories": {
    "doc": "docs",
    "example": "playground"
  },
  "keywords": [
    "meilisearch",
    "search",
    "vue-instantsearch",
    "nuxt",
    "module"
  ]
}
