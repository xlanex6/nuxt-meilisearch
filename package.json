{
  "name": "nuxt-meilisearch",
  "version": "0.1.9",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xlanex6/nuxt-meilisearch.git"
  },
  "author": "Alex Duval",
  "contributors": [],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs",
      "types": "./dist/types.d.ts"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "docs": "cd docus && pnpm dev",
    "release": "nuxt-module-build && standard-version && git push --follow-tags && npm publish",
    "lint": "eslint --ext .js,.ts,.vue"
  },
  "dependencies": {
    "@meilisearch/instant-meilisearch": "0.13.2",
    "@nuxt/kit": "3.5.3",
    "instantsearch.css": "8.0.0",
    "meilisearch": "0.33.0",
    "vue-instantsearch": "4.10.1"
  },
  "devDependencies": {
    "@nuxt/module-builder": "latest",
    "@nuxt/schema": "3.5.3",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/node": "20.2.5",
    "eslint": "latest",
    "nuxt": "3.5.3",
    "standard-version": "9.5.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://nuxt-meilisearch-docs.netlify.app/",
  "directories": {
    "doc": "docs",
    "example": "playground"
  },
  "keywords": [
    "meilisearch",
    "search",
    "vue-instantsearch",
    "nuxt",
    "module"
  ]
}
