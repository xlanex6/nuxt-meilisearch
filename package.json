{
  "name": "nuxt-meilisearch",
  "version": "0.1.12",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xlanex6/nuxt-meilisearch.git"
  },
  "author": "Alex Duval",
  "contributors": [],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "pnpm lint && bumpp && pnpm publish",
    "lint": "eslint .",
    "test": "vitest run",
    "test:types": "tsc --noEmit",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@meilisearch/instant-meilisearch": "0.13.5",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/kit": "3.7.3",
    "instantsearch.css": "8.1.0",
    "meilisearch": "0.34.2",
    "vue-instantsearch": "4.11.0"
  },
  "devDependencies": {
    "@nuxt/module-builder": "0.5.1",
    "@nuxt/schema": "3.7.3",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@types/node": "20.6.2",
    "bumpp": "9.2.0",
    "eslint": "^8.49.0",
    "nuxt": "3.7.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://nuxt-meilisearch-docs.netlify.app/",
  "directories": {
    "doc": "docs",
    "example": "playground"
  },
  "keywords": [
    "meilisearch",
    "search",
    "vue-instantsearch",
    "nuxt",
    "module"
  ],
  "packageManager": "pnpm@8.6.0"
}
