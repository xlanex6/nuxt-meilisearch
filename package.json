{
  "name": "nuxt-meilisearch",
  "type": "module",
  "version": "1.3.10",
  "author": "Alex Duval",
  "license": "MIT",
  "homepage": "https://nuxt-meilisearch.vercel.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xlanex6/nuxt-meilisearch.git"
  },
  "contributors": [],
  "keywords": [
    "meilisearch",
    "search",
    "vue-instantsearch",
    "nuxt",
    "module"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.mjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "pnpm run prepack && changelogen --release && pnpm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@meilisearch/instant-meilisearch": "0.27.0",
    "@nuxt/kit": "3.14.1592",
    "@nuxt/types": "^2.18.1",
    "instantsearch.css": "8.5.1",
    "meilisearch": "0.52.0",
    "vue-instantsearch": "4.21.2"
  },
  "devDependencies": {
    "@nuxt/devtools": "1.6.3",
    "@nuxt/eslint-config": "0.7.2",
    "@nuxt/module-builder": "0.8.4",
    "@nuxt/schema": "3.14.1592",
    "@nuxt/test-utils": "3.15.1",
    "@nuxt/ui": "2.19.2",
    "@types/node": "22.10.1",
    "changelogen": "0.5.7",
    "defu": "6.1.4",
    "eslint": "9.16.0",
    "nuxt": "3.14.1592",
    "typescript": "latest",
    "vitest": "2.1.8",
    "vue-tsc": "2.1.10"
  },
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "doc": "docs",
    "example": "playground"
  }
}
